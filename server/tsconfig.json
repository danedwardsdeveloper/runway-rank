{
	"compilerOptions": {
		"target": "ES2020",
		"module": "NodeNext",
		"rootDir": "./src",
		"outDir": "./dist/server",
		"esModuleInterop": true,
		"strict": true,
		"resolveJsonModule": true,
		"moduleResolution": "NodeNext",
		"baseUrl": "."
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "__tests__", "dist"]
}
